<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/borda_separa_fragmento"
    android:minWidth="550dp">

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/dialogo_qualidade_verion_botao_cancelar"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="32dp"

        android:adjustViewBounds="false"
        android:backgroundTintMode="src_in"
        android:scaleType="fitXY"
        android:src="@drawable/botao_cancelar_registros"
        app:backgroundTint="@color/secondaryDarkColor"
        app:fabCustomSize="60dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dialogo_qualidade_verion_nome_desvio_padrao_p2"
        app:maxImageSize="60dp" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/dialogo_qualidade_verion_botao_registrar"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginEnd="100dp"
        android:adjustViewBounds="false"
        android:backgroundTint="@color/primaryLightColor"
        android:backgroundTintMode="src_in"
        android:scaleType="fitXY"
        android:src="@drawable/botao_finalizar_registro"
        app:backgroundTint="@color/primaryLightColor"
        app:fabCustomSize="60dp"
        app:fabSize="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/dialogo_qualidade_verion_botao_cancelar"
        app:maxImageSize="60dp" />

    <TextView
        android:id="@+id/dialogo_qualidade_verion_nome_media_p2"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:textColor="@color/primaryTextColor"
        android:textSize="20sp"
        android:textStyle="normal"
        app:layout_constraintStart_toEndOf="@+id/textView142"
        app:layout_constraintTop_toTopOf="@+id/textView142" />

    <TextView
        android:id="@+id/dialogo_qualidade_verion_nome_desvio_padrao_p2"
        android:layout_width="275dp"
        android:layout_height="wrap_content"
        android:textColor="@color/primaryTextColor"
        android:textSize="20sp"
        android:textStyle="normal"
        app:layout_constraintStart_toEndOf="@+id/textView144"
        app:layout_constraintTop_toTopOf="@+id/textView144" />

    <TextView
        android:id="@+id/dialogo_qualidade_verion_letra_item3"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:textColor="@color/primaryTextColor"
        android:textSize="20sp"
        android:textStyle="normal"
        app:layout_constraintStart_toEndOf="@+id/dialogo_qualidade_verion_media_p2"
        app:layout_constraintTop_toTopOf="@+id/dialogo_qualidade_verion_nome_media_p2" />

    <TextView
        android:id="@+id/textView142"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="24dp"
        android:text="Média - "
        android:textColor="@color/primaryTextColor"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dialogo_qualidade_verion_nome_desvio_padrao_p1" />

    <EditText
        android:id="@+id/dialogo_qualidade_verion_media_p2"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:digits="1 2 3 4 5 6 7 8 9 0"
        android:ems="10"
        android:imeOptions="actionNext"
        android:inputType="numberDecimal"
        android:maxLength="9"
        android:textColor="@color/primaryTextColor"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/dialogo_qualidade_verion_nome_media_p2"
        app:layout_constraintStart_toEndOf="@+id/dialogo_qualidade_verion_nome_media_p2"
        app:layout_constraintTop_toTopOf="@+id/dialogo_qualidade_verion_nome_media_p2" />

    <TextView
        android:id="@+id/dialogo_qualidade_verion_letra_item4"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:textColor="@color/primaryTextColor"
        android:textSize="20sp"
        android:textStyle="normal"
        app:layout_constraintStart_toEndOf="@+id/dialogo_qualidade_verion_desvio_padrao_p2"
        app:layout_constraintTop_toTopOf="@+id/dialogo_qualidade_verion_nome_desvio_padrao_p2" />

    <EditText
        android:id="@+id/dialogo_qualidade_verion_desvio_padrao_p1"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:digits="1 2 3 4 5 6 7 8 9 0"
        android:ems="10"
        android:imeOptions="actionNext"
        android:inputType="numberDecimal"
        android:maxLength="9"
        android:textColor="@color/primaryTextColor"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/dialogo_qualidade_verion_nome_desvio_padrao_p1"
        app:layout_constraintStart_toEndOf="@+id/dialogo_qualidade_verion_nome_desvio_padrao_p1"
        app:layout_constraintTop_toTopOf="@+id/dialogo_qualidade_verion_nome_desvio_padrao_p1" />

    <TextView
        android:id="@+id/textView144"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="24dp"
        android:text="Desvio Padrão - "
        android:textColor="@color/primaryTextColor"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dialogo_qualidade_verion_nome_media_p2" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Dados Do Sistema de Precisão"
        android:textColor="@color/primaryTextColor"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView131"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:text="Média - "
        android:textColor="@color/primaryTextColor"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/dialogo_qualidade_verion_nome_media_p1"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:textColor="@color/primaryTextColor"
        android:textSize="20sp"
        android:textStyle="normal"
        app:layout_constraintStart_toEndOf="@+id/textView131"
        app:layout_constraintTop_toTopOf="@+id/textView131" />

    <TextView
        android:id="@+id/dialogo_qualidade_verion_nome_desvio_padrao_p1"
        android:layout_width="275dp"
        android:layout_height="wrap_content"
        android:textColor="@color/primaryTextColor"
        android:textSize="20sp"
        android:textStyle="normal"
        app:layout_constraintStart_toEndOf="@+id/textView132"
        app:layout_constraintTop_toTopOf="@+id/textView132" />

    <TextView
        android:id="@+id/textView132"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="24dp"
        android:text="Desvio Padrão - "
        android:textColor="@color/primaryTextColor"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dialogo_qualidade_verion_nome_media_p1" />

    <EditText
        android:id="@+id/dialogo_qualidade_verion_media_p1"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:digits="1 2 3 4 5 6 7 8 9 0"
        android:ems="10"
        android:imeOptions="actionNext"
        android:inputType="numberDecimal"
        android:maxLength="9"
        android:textColor="@color/primaryTextColor"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/dialogo_qualidade_verion_nome_media_p1"
        app:layout_constraintStart_toEndOf="@+id/dialogo_qualidade_verion_nome_media_p1"
        app:layout_constraintTop_toTopOf="@+id/dialogo_qualidade_verion_nome_media_p1" />

    <EditText
        android:id="@+id/dialogo_qualidade_verion_desvio_padrao_p2"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:digits="1 2 3 4 5 6 7 8 9 0"
        android:ems="10"
        android:imeOptions="actionNext"
        android:inputType="numberDecimal"
        android:maxLength="9"
        android:textColor="@color/primaryTextColor"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/dialogo_qualidade_verion_nome_desvio_padrao_p2"
        app:layout_constraintStart_toEndOf="@+id/dialogo_qualidade_verion_nome_desvio_padrao_p2"
        app:layout_constraintTop_toTopOf="@+id/dialogo_qualidade_verion_nome_desvio_padrao_p2" />

    <TextView
        android:id="@+id/dialogo_qualidade_verion_letra_item1"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:textColor="@color/primaryTextColor"
        android:textSize="20sp"
        android:textStyle="normal"
        app:layout_constraintStart_toEndOf="@+id/dialogo_qualidade_verion_media_p1"
        app:layout_constraintTop_toTopOf="@+id/dialogo_qualidade_verion_nome_media_p1" />

    <TextView
        android:id="@+id/dialogo_qualidade_verion_letra_item2"
        android:layout_width="40dp"
        android:layout_height="25dp"
        android:textColor="@color/primaryTextColor"
        android:textSize="20sp"
        android:textStyle="normal"
        app:layout_constraintStart_toEndOf="@+id/dialogo_qualidade_verion_desvio_padrao_p1"
        app:layout_constraintTop_toTopOf="@+id/dialogo_qualidade_verion_nome_desvio_padrao_p1" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/borda_separa_fragmento"
        app:layout_constraintBottom_toTopOf="@+id/dialogo_qualidade_verion_botao_registrar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView">

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>