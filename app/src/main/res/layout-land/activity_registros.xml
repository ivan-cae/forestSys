        <TextView
            android:id="@+id/textView71"
            android:layout_width="35dp"
            android:layout_height="20dp"
            android:background="#BDB8B8"
            android:text="H.O.E"
            android:textAlignment="center"
            android:textSize="12sp"
            android:textStyle="normal"
            app:layout_constraintStart_toEndOf="@+id/textView65"
            app:layout_constraintTop_toTopOf="@+id/textView65" />

        <TextView
            android:id="@+id/textView68"
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:background="#BDB8B8"
            android:text="H.A."
            android:textAlignment="center"
            android:textSize="12sp"
            android:textStyle="normal"
            app:layout_constraintStart_toEndOf="@+id/textView76"
            app:layout_constraintTop_toTopOf="@+id/textView76" />

        <TextView
            android:id="@+id/textView70"
            android:layout_width="55dp"
            android:layout_height="20dp"
            android:layout_marginTop="8dp"
            android:background="#BDB8B8"
            android:text="Data"
            android:textAlignment="center"
            android:textSize="10sp"
            android:textStyle="normal"
            app:layout_constraintStart_toStartOf="@+id/registro_fragmento_rendimento"
            app:layout_constraintTop_toBottomOf="@+id/registro_fragmento_rendimento" />

        <TextView
            android:id="@+id/textView76"
            android:layout_width="35dp"
            android:layout_height="20dp"
            android:background="#BDB8B8"
            android:text="Area"
            android:textAlignment="center"
            android:textSize="12sp"
            android:textStyle="normal"
            app:layout_constraintStart_toEndOf="@+id/textView70"
            app:layout_constraintTop_toTopOf="@+id/textView70" />

        <TextView
            android:id="@+id/textView75"
            android:layout_width="82dp"
            android:layout_height="20dp"
            android:background="#BDB8B8"
            android:text="Responsável"
            android:textAlignment="center"
            android:textSize="12sp"
            android:textStyle="normal"
            app:layout_constraintStart_toEndOf="@+id/textView78"
            app:layout_constraintTop_toTopOf="@+id/textView78" />

        <TextView
            android:id="@+id/registros_insumo2"
            android:layout_width="85dp"
            android:layout_height="20dp"
            android:background="#BDB8B8"
            android:textAlignment="center"
            android:textSize="12sp"
            android:textStyle="normal"
            app:layout_constraintStart_toEndOf="@+id/registros_insumo1"
            app:layout_constraintTop_toTopOf="@+id/registros_insumo1" />

        <TextView
            android:id="@+id/registros_insumo1"
            android:layout_width="85dp"
            android:layout_height="20dp"
            android:background="#BDB8B8"
            android:textAlignment="center"
            android:textSize="12sp"
            android:textStyle="normal"
            app:layout_constraintStart_toEndOf="@+id/textView63"
            app:layout_constraintTop_toTopOf="@+id/textView63" />

        <TextView
            android:id="@+id/textView78"
            android:layout_width="82dp"
            android:layout_height="20dp"
            android:background="#BDB8B8"
            android:text="Prestador"
            android:textAlignment="center"
            android:textSize="12sp"
            android:textStyle="normal"
            app:layout_constraintStart_toEndOf="@+id/registros_insumo2"
            app:layout_constraintTop_toTopOf="@+id/registros_insumo2" />

        <TextView
            android:id="@+id/textView74"
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:background="#BDB8B8"
            android:text="H.O."
            android:textAlignment="center"
            android:textSize="12sp"
            android:textStyle="normal"
            app:layout_constraintStart_toEndOf="@+id/textView68"
            app:layout_constraintTop_toTopOf="@+id/textView68" />

        <TextView
            android:id="@+id/data_apontamento"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/textView81"
            app:layout_constraintTop_toTopOf="@+id/textView81" />

        <ImageButton
            android:id="@+id/botao_continuar_voltar"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:background="#FFFFFF"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/ic_voltar_24dp" />

        <TextView
            android:id="@+id/status_os_continuar"
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/textView42"
            app:layout_constraintTop_toTopOf="@+id/textView42" />

        <TextView
            android:id="@+id/area_prog_os_continuar"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/textView46"
            app:layout_constraintTop_toTopOf="@+id/textView46" />

        <TextView
            android:id="@+id/area_realizada_os_continuar"
            android:layout_width="80dp"
            android:layout_height="0dp"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/textView101"
            app:layout_constraintTop_toTopOf="@+id/textView101" />

        <TextView
            android:id="@+id/talhao_os_continuar"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/textView20"
            app:layout_constraintTop_toTopOf="@+id/textView20" />

        <TextView
            android:id="@+id/textView81"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="70dp"
            android:text="Data:"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/status_os_continuar"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView42"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="70dp"
            android:text="Status:"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/area_realizada_os_continuar"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView46"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="70dp"
            android:text="Área:"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/talhao_os_continuar"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView101"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="70dp"
            android:text="Área Realizada:"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/area_prog_os_continuar"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="70dp"
            android:text="Talhão:"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/botao_apontamentos_geral"
            android:layout_width="250dp"
            android:layout_height="40dp"
            android:layout_marginTop="8dp"
            android:text="Rendimento"
            android:textAlignment="center"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/registro_fragmento_rendimento"
            app:layout_constraintStart_toStartOf="@+id/registro_fragmento_rendimento"
            app:layout_constraintTop_toBottomOf="@+id/area_prog_os_continuar" />

        <Button
            android:id="@+id/botao_apontamentos_insumos"
            android:layout_width="250dp"
            android:layout_height="40dp"
            android:text="Insumos"
            android:textAlignment="center"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/registro_fragmento_insumos"
            app:layout_constraintStart_toStartOf="@+id/registro_fragmento_insumos"
            app:layout_constraintTop_toTopOf="@+id/botao_apontamentos_geral" />

        <ImageButton
            android:id="@+id/botao_date_picker"
            android:layout_width="41dp"
            android:layout_height="38dp"
            android:scaleType="center"
            app:layout_constraintBottom_toBottomOf="@+id/data_apontamento"
            app:layout_constraintStart_toEndOf="@+id/data_apontamento"
            app:layout_constraintTop_toTopOf="@+id/data_apontamento"
            app:srcCompat="@android:drawable/ic_menu_my_calendar" />

        <TextView
            android:id="@+id/textView94"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:text="Toque No Insumo Para Adicionar a Aplicação"
            android:textColor="#000000"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/registro_fragmento_insumos"
            app:layout_constraintStart_toStartOf="@+id/registro_fragmento_insumos"
            app:layout_constraintTop_toBottomOf="@+id/botao_apontamentos_insumos" />

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/registros_lista"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginBottom="15dp"
            app:layout_constraintBottom_toTopOf="@+id/botao_continuar_voltar"
            app:layout_constraintEnd_toEndOf="@+id/textView75"
            app:layout_constraintStart_toStartOf="@+id/textView70"
            app:layout_constraintTop_toBottomOf="@+id/textView65" />

        <FrameLayout
            android:id="@+id/registro_fragmento_rendimento"
            android:layout_width="480dp"
            android:layout_height="240dp"
            android:layout_marginStart="8dp"
            android:background="#FFFFFF"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/botao_apontamentos_geral">

        </FrameLayout>

        <FrameLayout
            android:id="@+id/registro_fragmento_insumos"
            android:layout_width="350dp"
            android:layout_height="280dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:background="#FFFFFF"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/registros_lista"
            app:layout_constraintTop_toBottomOf="@+id/textView94">

        </FrameLayout>

        <FrameLayout
            android:id="@+id/fragment_container_continuar"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="visible"
            tools:layout_editor_absoluteX="15dp"
            tools:layout_editor_absoluteY="98dp">

            <LinearLayout
                android:id="@+id/linear_layout_continuar"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:visibility="visible">

                <androidx.appcompat.widget.Toolbar
                    android:id="@+id/toolbar_continuar"
                    android:layout_width="match_parent"
                    android:layout_height="?attr/actionBarSize"
                    android:background="#4183B1"
                    android:elevation="4dp"
                    android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
                    android:visibility="visible"
                    app:popupTheme="@style/ThemeOverlay.AppCompat.Light" />
            </LinearLayout>

        </FrameLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <com.google.android.material.navigation.NavigationView
        android:id="@+id/nav_view_continuar"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:maxWidth="230dp"
        android:visibility="visible"
        app:menu="@menu/menu_main_lateral" />

</androidx.drawerlayout.widget.DrawerLayout>

