<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="60dp"
    android:background="@drawable/custom_ripple">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/bordas"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/manejo_item_lista"
            android:layout_width="110dp"
            android:layout_height="25dp"
            android:layout_marginTop="17.5dp"
            android:textAlignment="center"
            android:textColor="@color/primaryTextColor"
            android:textSize="18sp"
            android:textStyle="normal"
            app:layout_constraintStart_toEndOf="@+id/area_item_lista"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/setor_item_lista"
            android:layout_width="120dp"
            android:layout_height="25dp"
            android:layout_marginTop="17.5dp"
            android:textAlignment="center"
            android:textColor="@color/primaryTextColor"
            android:textSize="18sp"
            android:textStyle="normal"
            app:layout_constraintStart_toEndOf="@+id/data_item_lista"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/data_item_lista"
            android:layout_width="200dp"
            android:layout_height="25dp"
            android:layout_marginTop="17.5dp"
            android:textAlignment="center"
            android:textColor="@color/primaryTextColor"
            android:textSize="18sp"
            android:textStyle="normal"
            app:layout_constraintStart_toEndOf="@+id/status_item_lista"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/area_item_lista"
            android:layout_width="110dp"
            android:layout_height="25dp"
            android:layout_marginTop="17.5dp"
            android:textAlignment="center"
            android:textColor="@color/primaryTextColor"
            android:textSize="18sp"
            android:textStyle="normal"
            app:layout_constraintStart_toEndOf="@+id/talhao_item_lista"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/talhao_item_lista"
            android:layout_width="150dp"
            android:layout_height="25dp"
            android:layout_marginTop="17.5dp"
            android:textAlignment="center"
            android:textColor="@color/primaryTextColor"
            android:textSize="18sp"
            android:textStyle="normal"
            app:layout_constraintStart_toEndOf="@+id/setor_item_lista"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/status_item_lista"
            android:layout_width="125dp"
            android:layout_height="25dp"
            android:layout_marginTop="17.5dp"
            android:textAlignment="center"
            android:textColor="@color/primaryTextColor"
            android:textSize="18sp"
            android:textStyle="normal"
            app:layout_constraintStart_toEndOf="@+id/icone_status"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/madeira_item_lista"
            android:layout_width="120dp"
            android:layout_height="25dp"
            android:layout_marginTop="17.5dp"
            android:textAlignment="center"
            android:textColor="@color/primaryTextColor"
            android:textSize="18sp"
            android:textStyle="normal"
            app:layout_constraintStart_toEndOf="@+id/manejo_item_lista"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/icone_status"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginTop="17dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>

